<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sanofood.controllers.Adduser">
   <children>
      <TextField fx:id="email" layoutX="218.0" layoutY="25.0" promptText="email" />
      <TextField fx:id="name" layoutX="218.0" layoutY="63.0" promptText="name" />
      <TextField fx:id="lastname" layoutX="218.0" layoutY="101.0" promptText="lastname" />
      <Button layoutX="231.0" layoutY="200.0" mnemonicParsing="false" onAction="#save" prefHeight="51.0" prefWidth="124.0" text="Signup" />
      <PasswordField fx:id="pass" layoutX="218.0" layoutY="148.0" promptText="password" />
      <Button layoutX="219.0" onAction="#goToLogin" layoutY="271.0" mnemonicParsing="false" text="Already have an account" />
   </children>
</AnchorPane>
